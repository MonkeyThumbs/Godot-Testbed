#		state = FAILED
#	if not owner.check_can_attack():
#		tick.blackboard.set(\"last_known_player_location\", owner.get_player_position())
#		state = FAILED
	if not is_done or not owner.check_is_on_floor():
		state = ERR_BUSY
	
	return state


func _on_AnimationPlayer_animation_finished(anim_name):
	if anim_name == \"attack\":
		is_done = true
